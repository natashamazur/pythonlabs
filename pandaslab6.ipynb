      "source": [
        "str_test = 'abc bcb be bebb'\n",
        " \n",
        "s = pd.Series(list(str_test))\n",
        "freq = s.value_counts()\n",
        "least_freq = freq.dropna().index[-1]\n",
        "''.join(s.replace(' ', least_freq))\n"
