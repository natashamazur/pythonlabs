        "outputId": "5caa4c1d-8e30-4490-cf16-e39c5b01c6b4"
      },
      "source": [
        "n = 8\n",
        " \n",
        "s = pd.Series(np.arange(n))\n",
        "s = s + np.random.normal(1, 3, n)\n",
        " \n",
        "autocorr = [s.autocorr(lag=i).round(2) for i in range(n)]\n",
        " \n",
        "print(autocorr)"
      ],
